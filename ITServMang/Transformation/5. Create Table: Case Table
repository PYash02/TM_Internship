DROP TABLE IF EXISTS _ITSM_CASE_TABLE; 
CREATE TABLE _ITSM_CASE_TABLE ( 
INC_NUMBER VARCHAR(15) 
, PRIORITY VARCHAR(15) 
, URGENCY VARCHAR(15) 
, LOCATION VARCHAR(100) 
, DEPARTMENT VARCHAR(100) 
, INCIDENT_STATUS VARCHAR(15) 
, INCIDENT_CREATE_TIME timestamp 
, INCIDENT_OPEN_TIME timestamp 
, INCIDENT_UPDATE_TIME timestamp 
, INCIDENT_RESOLVE_TIME timestamp 
); 

select * from _ITSM_CASE_TABLE;
