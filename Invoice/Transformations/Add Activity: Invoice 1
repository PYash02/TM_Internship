/*

DELETE FROM _INVOICE_ACTIVITIES;

*/

INSERT INTO _INVOICE_ACTIVITIES (

_CASE_KEY

, _ACTIVITY_EN

, _EVENTTIME

, _COUNTRY

, _TEAM

, _USER

, _SORTING

)

SELECT

Invoice1_Sheet1.CASE_ID AS _CASE_KEY, Invoice1_Sheet1.ACTIVITY AS _ACTIVITY_EN, Invoice1_Sheet1.ENTRYDATE_FINAL AS _EVENTTIME,

Invoice1_Sheet1.COUNTRY AS _COUNTRY, Invoice1_Sheet1.TEAM AS _TEAM, Invoice1_Sheet1.USER AS _USER,

20 AS _SORTING

FROM Invoice1_Sheet1 JOIN User_Sheet1 ON 1=1

AND "User_Sheet1"."USER" = "Invoice1_Sheet1"."USER" JOIN Invoice_Cases ON 1=1

AND "Invoice_Cases"."CASE_ID" = "Invoice1_Sheet1"."CASE_ID"

WHERE "Invoice1_Sheet1"."CASE_ID" IN (SELECT DISTINCT("Invoice_Cases"."CASE_ID") FROM "Invoice_Cases");



SELECT COUNT(*) FROM "_INVOICE_ACTIVITIES" WHERE _ACTIVITY_EN = 'Check received invoice';
