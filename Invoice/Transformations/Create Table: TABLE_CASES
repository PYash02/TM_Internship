DROP TABLE IF EXISTS TABLE_CASES; CREATE TABLE TABLE_CASES AS SELECT

Invoice_Cases.SUPPLIER_TYPE

,Invoice_Cases.SUPPLIER

,Invoice_Cases.CASE_TYPE

,Invoice_Cases.CASE_OWNER

,Invoice_Cases.CASE

,Invoice_Cases.AMOUNT

,Invoice_Cases.CASE_ID AS _CASE_KEY FROM Invoice_Cases;



SELECT COUNT(*) FROM TABLE_CASES WHERE CASE_OWNER IS NULL; SELECT COUNT(DISTINCT(_CASE_KEY)) FROM TABLE_CASES;

SELECT COUNT(DISTINCT(CASE_OWNER)) FROM TABLE_CASES;



SELECT COUNT(*) FROM _INVOICE_ACTIVITIES;

SELECT COUNT(DISTINCT(_CASE_KEY)) FROM _INVOICE_ACTIVITIES;



SELECT COUNT("TABLE_CASES"."_CASE_KEY") FROM TABLE_CASES, _INVOICE_ACTIVITIES
